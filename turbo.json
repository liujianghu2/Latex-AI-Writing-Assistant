{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", ".wrangler/**", "dist/**"],
      "env": [
        "OPENAI_API_KEY",
        "LATEX_API_URL",
        "KV_REST_API_URL",
        "KV_REST_API_TOKEN"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  },
  "globalEnv": [],
  "globalDependencies": [".env", ".env.local"]
}
